{% extends 'base.html.twig' %}

{% block title %} Résultats {% endblock %}

{% block body %}
	<div class="text-center mt-5">
		<h5 class="mb-3"><u> Résultat des cartes triées : </u></h5>
		<ol>
			{% for card in cards %}
				<li> {{ card.category|capitalize }} => {{ card.value|capitalize }} </li>
			{% endfor %}
		</ol>
	</div>
{% endblock %}